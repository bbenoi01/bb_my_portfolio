        <div id='contact'>
            <div class='section'>
                <div class='row'>
                    <div class='col s12 center'>
                        <h4>Contact Me</h4>
                        <div class='container'>
                            <form>
                                <div class='row'>
                                    <div class='col s3'>
                                        <input type="text" name="firstName" class='disabled'>
                                        <label for="firstName">First Name</label>
                                    </div>
                                    <div class='col s3'>
                                        <input type="text" name="lastName" class='disabled'>
                                        <label for="lastName">Last Name</label>
                                    </div>
                                    <div class='col s3'>
                                        <input type="text" name="email" class='disabled'>
                                        <label for="email">Email</label>
                                    </div>
                                    <div class='col s3'>
                                        <button class="btn blue waves-effect waves-light disabled" type="submit">Submit</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class='parallax-container valign-wrapper'>
            <div class='section no-pad-bot'>
                <div class='container'>
                    <div class='row center'>
                        <h3 class='yellow-text text-accent-2' id='thanks'>Thank you <%= thankYou.firstName %> <%= thankYou.lastName %>, for your interest. I will reach out to you at <%= thankYou.email %> shortly.</h3>
